{
  "private": true,
  "description": "A powerful micro front-end framework",
  "scripts": {
    "dev": "pnpm build:watch & node scripts/dev.js",
    "dev:docs": "pnpm run start --filter garfish-docs",
    "test:e2e": "zx scripts/e2e.js",
    "test:e2e:open": "cross-env TEST_ENV_OPEN=true zx scripts/e2e.js",
    "test": "node scripts/jestTest.js",
    "test:cover": "node scripts/jestTest.js --coverage",
    "cy:open": "cypress open",
    "cy:run": "cypress run",
    "changeset": "npx changeset",
    "build": "pnpm run build --parallel --filter @garfish/* --filter garfish",
    "build:docs": "pnpm run build --filter garfish-docs",
    "build:watch": "cross-env WATCH=true pnpm build",
    "reset": "pnpm -r exec -- rm -rf node_modules && rm -rf node_modules",
    "setup": "pnpm install",
    "prepublishOnly": "pnpm test && pnpm build",
    "postpublish": "node scripts/asyncBnpm.js",
    "ls-lint": "ls-lint",
    "lint": "eslint --ext .ts packages/*/*/src/**",
    "reset-gittag": "git tag -l | xargs git tag -d && git fetch --prune",
    "format": "prettier --write --parser typescript \"packages/**/*.ts?(x)\"",
    "format:js": "prettier --write --parser typescript \"(packages|scripts)/**/*.js\"",
    "format:md": "prettier --write \"**/*.md\"",
    "bump": "node ./scripts/versionBump.js",
    "release": "node ./scripts/release.js",
    "release:canary": "node ./scripts/release.js --canary"
  },
  "husky": {
    "hooks": {
      "pre-commit": "ls-lint && lint-staged",
      "commit-msg": "node scripts/verifyCommit.js"
    }
  },
  "lint-staged": {
    "*.js": [
      "prettier --write"
    ],
    "*.ts?(x)": [
      "eslint --fix",
      "prettier --parser=typescript --write"
    ],
    "*.md": [
      "prettier --write"
    ]
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org"
  },
  "author": "bytedance",
  "license": "Apache-2.0",
  "devDependencies": {
    "@babel/plugin-transform-regenerator": "^7.12.1",
    "@babel/preset-env": "^7.12.7",
    "@babel/preset-typescript": "^7.12.7",
    "@jsdevtools/version-bump-prompt": "^6.1.0",
    "@ls-lint/ls-lint": "^1.9.2",
    "@types/jest": "^26.0.15",
    "@typescript-eslint/parser": "^3.10.1",
    "@vue/cli-plugin-babel": "~4.4.0",
    "@vue/cli-plugin-eslint": "~4.4.0",
    "@vue/cli-service": "~4.4.0",
    "axios": "^0.21.1",
    "babel-polyfill": "^6.26.0",
    "chalk": "^4.1.0",
    "cross-env": "^7.0.3",
    "cypress": "^8.0.0",
    "esbuild-plugin-replace": "^1.0.7",
    "eslint": "^7.14.0",
    "eslint-plugin-vue": "^6.2.2",
    "execa": "^4.1.0",
    "fs-extra": "^9.0.1",
    "http-server": "^0.12.3",
    "husky": "^4.3.0",
    "install": "^0.13.0",
    "jest": "^26.6.3",
    "kill-port": "^1.6.1",
    "lint-staged": "^10.5.2",
    "lodash": "^4.17.21",
    "minimist": "^1.2.5",
    "pnpm": "^6.15.1",
    "prettier": "~2.0.5",
    "prettier-eslint-cli": "^5.0.0",
    "rimraf": "^3.0.2",
    "ts-jest": "^26.4.4",
    "tslib": "^2.3.1",
    "tsup": "^5.1.0",
    "typescript": "^4.4.3",
    "wait-on": "^6.0.0",
    "workspace-tools": "^0.16.2",
    "zx": "^4.2.0"
  },
  "version": "0.1.15"
}
